// function arguments

function multiply() {
  let total = 1
  for (const argument of arguments) {
    total *= argument
    // console.log(argument)
  }
  return total
}

console.log(multiply(1, 2, 3, 4))
console.log(multiply(1, 2, 3, 4, 5, 6, 7))
// console.log(multiply(1, 2, 3, 4, 5, 6, 7, 21, 3, 213, 12, 312, 32, 432, 4, 23, 423, 432423, 432, 423, 4, 23, 42, 3, 432, 32))