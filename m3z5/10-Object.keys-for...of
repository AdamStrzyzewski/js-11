// Object.keys for...of
const book = {
  title: "The Last Kingdom",
  author: "Bernard Cornwell",
  genres: ["historical prose", "adventure"],
  rating: 8.38,
};

const keys = Object.keys(book);
console.log(keys);
console.log('-----')
for (const key of keys) {
  console.log(`inside of for...of ${key}`)
}