// Object.entries
const book = {
  title: "The Last Kingdom",
  author: "Bernard Cornwell",
  genres: ["historical prose", "adventure"],
  rating: 8.38,
};

console.log('------KEYS--------')
console.log(Object.keys(book))
console.log('------VALUES--------')
console.log(Object.values(book))
console.log('------ENTRIES--------')
console.log(Object.entries(book))

