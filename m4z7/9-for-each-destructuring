// for each destructuring

const users = [{
    name: "Adam",
    rating: 5
  },
  {
    name: "Oskar",
    rating: 7
  },
  {
    name: "Åukasz",
    rating: 2
  },
  {
    name: "Filip",
    rating: 8
  },
  {
    name: "Tomek",
    rating: 3
  },
]


let totalRating = 0
users.forEach(({
  name,
  rating
}) => {
  if (rating > 3) {
    console.log(name)
  }
  totalRating += rating
})

console.log(`Average rating is ${totalRating / users.length}`)