// reduce 1

const array = [23, 432, 532]


const total = array.reduce(
  // 1 iteracja, acc = 0, el = 23 -> zwracam 0 + 23
  // 2 iteracja, acc = 23, el = 432 -> zwracam 23 + 432
  // 3 iteracja, acc = 455, el = 532 -> zwracam 455 + 532
  // koniec tablicy, nastÄ™puje zwrotka acc === 987
  (acc, el) => acc + el,
  0
)

console.log(`total: ${total}`)