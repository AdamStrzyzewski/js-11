// spread... in a function array as arguments

const userInput = '15,13424,0,-4121,4,12,57,23,23,86,323';

const parsed = userInput.split(",")

for (let i = 0; i < parsed.length; i += 1) {
  parsed[i] = Number.parseInt(parsed[i])
}

